generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Ride {
  id          String @id @default(uuid())
  name        String
  description String
  car         String
  rating      Float
  review      String
  pricePerKm  Int
  minKm       Int
}
